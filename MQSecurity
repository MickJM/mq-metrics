setmqaut -m TSTQPD01 -t qmgr -g MQMon +connect +inq +alladm
setmqaut -m TSTQPD01 -n "**" -t q -g MQMon +alladm +crt +browse
setmqaut -m TSTQPD01 -n "**" -t topic -g MQMon +alladm +crt
setmqaut -m TSTQPD01 -n "**" -t channel -g MQMon +alladm +crt
setmqaut -m TSTQPD01 -n "**" -t process -g MQMon +alladm +crt
setmqaut -m TSTQPD01 -n "**" -t namelist -g MQMon +alladm +crt
setmqaut -m TSTQPD01 -n "**" -t authinfo -g MQMon +alladm +crt
setmqaut -m TSTQPD01 -n "**" -t clntconn -g MQMon +alladm +crt
setmqaut -m TSTQPD01 -n "**" -t listener -g MQMon +alladm +crt
setmqaut -m TSTQPD01 -n "**" -t service -g MQMon +alladm +crt
setmqaut -m TSTQPD01 -n "**" -t comminfo -g MQMon +alladm +crt

Rem The following commands provide administrative access for MQ Explorer.
rem setmqaut -m TSTQPD01 -n SYSTEM.MQEXPLORER.REPLY.MODEL -t q -g "MQMon" +dsp +inq +get
rem setmqaut -m TSTQPD01 -n SYSTEM.ADMIN.COMMAND.QUEUE -t q -g "MQMon" +dsp +inq +put


setmqaut -m AOKQSC01 -t qmgr -g MQMon01 +connect +inq +alladm
setmqaut -m AOKQSC01 -n "**" -t q -g MQMon01 +alladm +all
setmqaut -m AOKQSC01 -n "**" -t topic -g MQMon01 +alladm +crt
setmqaut -m AOKQSC01 -n "**" -t channel -g MQMon01 +alladm +crt
setmqaut -m AOKQSC01 -n "**" -t process -g MQMon01 +alladm +crt
setmqaut -m AOKQSC01 -n "**" -t namelist -g MQMon01 +alladm +crt
setmqaut -m AOKQSC01 -n "**" -t authinfo -g MQMon01 +alladm +crt
setmqaut -m AOKQSC01 -n "**" -t clntconn -g MQMon01 +alladm +crt
setmqaut -m AOKQSC01 -n "**" -t listener -g MQMon01 +alladm +crt
setmqaut -m AOKQSC01 -n "**" -t service -g MQMon01 +alladm +crt
setmqaut -m AOKQSC01 -n "**" -t comminfo -g MQMon01 +alladm +crt

Rem The following commands provide administrative access for MQ Explorer.
rem setmqaut -m AOKQSC01 -n SYSTEM.MQEXPLORER.REPLY.MODEL -t q -g "MQMon01" +dsp +inq +get
rem setmqaut -m AOKQSC01 -n SYSTEM.ADMIN.COMMAND.QUEUE -t q -g "MQMon01" +dsp +inq +put

setmqaut -m AOKQSC01 -n SYSTEM.ADMIN.COMMAND.QUEUE -t q -g "MQMon01" +dsp +inq +put
setmqaut -m AOKQSC01 -n SYSTEM.MQEXPLORER.REPLY.MODEL -t q -g "MQMon01" +dsp +inq +get


runmqckm -cert -list -db TSTQPD01key.kdb -stashed

runmqckm -cert -details -db TSTQPD01key.kdb -stashed -label MQ_CA

